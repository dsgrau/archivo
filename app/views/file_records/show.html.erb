<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @file_record.title %>
</p>

<% @file_record.steps.each do |step| %>
  <%= step.office.name %> </br>
  <% end %>

<%= form_for[@file_record, @step] do |f| %>
  <%= f.colection_select(:step, :office_id, Office.all, :id, :name) %>
  <%= f.submit %>
<%= end %>

<%= link_to 'Edit', edit_file_record_path(@file_record) %> |
<%= link_to 'Back', file_records_path %>
